
<Files ~ ".*$">
    Order deny,allow
    Deny from all
</Files>

<Files ~ ".*.php$">
    Order allow,deny
    Allow from all
</Files>

<Files ~ ".*.gif$">
    Order allow,deny
    Allow from all
</Files>

<Files ~ ".*.swf$">
    Order allow,deny
    Allow from all
</Files>

<Files ~ ".*.png$">
    Order allow,deny
    Allow from all
</Files>

<Files ~ ".*.svg$">
    Order allow,deny
    Allow from all
</Files>

<Files ~ ".*.jpg$">
    Order allow,deny
    Allow from all
</Files>
